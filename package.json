{
  "name": "dialogflow-chat-widget",
  "description": "A web chat widget web component that interacts with a dialogflow interactions api to detect intents",
  "version": "0.0.1",
  "author": "Emmanuel Asamoah Botwe",
  "license": "BSD-2-Clause",
  "homepage": "https://github.com/botchway44/dialogflow-chat-widget#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/botchway44/dialogflow-chat-widget.git"
  },
  "bugs": {
    "url": "https://github.com/botchway44/dialogflow-chat-widget/issues"
  },
  "keywords": [
    "chat",
    "chat-widget",
    "dialogflow",
    "interations-api",
    "web-components",
    "webcomponents"
  ],
  "type": "module",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "jsdelivr": "dist/dialogflow-chat-widget.js",
  "unpkg": "dist/dialogflow-chat-widget.min.js",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "index.html"
  ],
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsc",
    "build:widget": "rollup -c && tsc -d --emitDeclarationOnly --declarationDir dist/esm",
    "start:dev": "rollup -c --watch --plugin dev",
    "start": "harp . --port $PORT",
    "build:doc": "wca src/widget/index.ts --outFiles doc/{tagname}.md"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-modules-umd": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^13.0.2",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/google.maps": "^3.45.6",
    "@types/googlemaps": "^3.30.8",
    "@types/node": "^9.6.61",
    "@types/rollup-plugin-auto-external": "^2.0.2",
    "@types/uuid": "^8.3.1",
    "focus-visible": "^5.2.0",
    "path": "^0.12.7",
    "rollup": "^2.53.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-commonjs": "^8.4.1",
    "rollup-plugin-dev": "^1.1.3",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-postcss": "^1.3.3",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-string": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.2",
    "tslib": "^2.3.0",
    "typescript": "^3.9.10",
    "web-component-analyzer": "^1.1.6"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.12.1",
    "harp": "*",
    "@google-cloud/dialogflow-cx": "^2.12.0",
    "mongodb": "^3.1.12",
    "dotenv": "^10.0.0",
    "body-parser": "^1.19.0"

  }
}
